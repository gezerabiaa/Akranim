<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AKRANIM.AnaMenu1">


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="79dp"
        android:layout_height="53dp"
        android:src="@drawable/rectangle_5"
        app:layout_constraintEnd_toEndOf="@+id/ileriButton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:scaleX="6"
        android:scaleY="1.8"/>

    <ImageButton
        android:id="@+id/ileriButton"
        android:layout_width="52dp"
        android:layout_height="52dp"
        android:layout_marginBottom="8dp"
        android:src="@drawable/back_icon_2"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/geriButton"
        android:layout_width="53dp"
        android:layout_height="53dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="7dp"
        android:src="@drawable/back_icon_1"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.034"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="446dp"
        android:layout_height="209dp"
        android:layout_marginTop="4dp"
        android:scaleX="1.14"
        android:src="@drawable/image_1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.514"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/girisText"
        app:layout_constraintVertical_bias="0.012" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="379dp"
        android:layout_height="191dp"
        android:layout_marginStart="51dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="51dp"
        android:scaleX="1.8"
        android:scaleY="1.6"
        android:src="@drawable/rectangle_15"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <ImageButton
        android:id="@+id/takvimButton"
        android:layout_width="76dp"
        android:layout_height="73dp"
        android:layout_marginBottom="84dp"
        android:scaleType="centerInside"
        android:src="@drawable/takvim1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/girisText"
        android:layout_width="281dp"
        android:layout_height="55dp"
        android:layout_marginStart="13dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="3dp"
        android:text="Yeditepe Üniversitesi Hakkında"
        android:textAlignment="center"
        android:textColor="#2C6C2D"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/ileriButton"
        app:layout_constraintHorizontal_bias="0.866"
        app:layout_constraintStart_toEndOf="@+id/geriButton"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="342dp"
        android:layout_height="265dp"
        android:layout_marginBottom="120dp"
        android:text="İstanbul Eğitim ve Kültür Vakfı (İSTEK) tarafından 1996 yılında kurulan Yeditepe Üniversitesi; Türkiye’de birçok alanda öncü, seçkin, akademik ve yönetim kadroları ile Atatürkçü düşüncelerin ışığında çağdaş, araştırmacı, yenilikçi, iş dünyasının ihtiyaç duyduğu tüm nitelik ve donanıma sahip, teknolojiye hakim, dünya kültürünü tanıyan, öz benliğini güçlendirmiş gençler yetiştirmeyi amaçlıyor. Üniversitemiz, çağdaş akademik program ve uygulamalarıyla bilgi çağına dönük eğitim yapıyor.  "
        android:textAlignment="center"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/locationButton"
        android:layout_width="67dp"
        android:layout_height="64dp"
        android:layout_marginBottom="16dp"
        android:src="@drawable/konum"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.651"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/kulupButton"
        android:layout_width="73dp"
        android:layout_height="64dp"
        android:layout_marginBottom="16dp"
        android:src="@drawable/club"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.079"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/profileButton"
        android:layout_width="73dp"
        android:layout_height="67dp"
        android:layout_marginBottom="16dp"
        android:src="@drawable/profil"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.952"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/chatButton"
        android:layout_width="71dp"
        android:layout_height="65dp"
        android:layout_marginBottom="16dp"
        android:src="@drawable/livechat"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.361"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>