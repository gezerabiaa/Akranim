<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context=".AKRANIM.AnaMenu2">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="79dp"
        android:layout_height="53dp"
        android:src="@drawable/mavirectangle_5__1_"
        app:layout_constraintEnd_toEndOf="@+id/ileriButton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:scaleX="6"
        android:scaleY="1.8"/>

    <ImageButton
        android:id="@+id/ileriButton"
        android:layout_width="52dp"
        android:layout_height="52dp"
        android:layout_marginBottom="8dp"
        android:src="@drawable/back_icon_2"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/geriButton"
        android:layout_width="53dp"
        android:layout_height="53dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="7dp"
        android:src="@drawable/back_icon_1"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.034"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="446dp"
        android:layout_height="209dp"
        android:layout_marginTop="4dp"
        android:scaleX="1.1"
        android:src="@drawable/rektorluk_2_1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.514"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/girisText"
        app:layout_constraintVertical_bias="0.012" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="379dp"
        android:layout_height="191dp"
        android:layout_marginStart="51dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="51dp"
        android:scaleX="1.8"
        android:scaleY="1.6"
        android:src="@drawable/mavirectangle_15__1_"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <TextView
        android:id="@+id/girisText"
        android:layout_width="281dp"
        android:layout_height="55dp"
        android:layout_marginStart="13dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="3dp"
        android:text="Yeditepe Üniversitesi Hakkında"
        android:textAlignment="center"
        android:textColor="#2C6C2D"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/ileriButton"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toEndOf="@+id/geriButton"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.007" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="342dp"
        android:layout_height="265dp"
        android:layout_marginBottom="132dp"
        android:text="Yeditepe Üniversitesi akademik faaliyetlerini Diş Hekimliği, Eğitim, Fen-Edebiyat, Güzel Sanatlar, Hukuk, İletişim, Mimarlık, Mühendislik, Tıp ve Ticari Bilimler olmak üzere 13 Fakülte, 2 Yüksekokul ve 1 Meslek Yüksekokulu’nda toplam 67 programda lisans ve 4 ön lisans programı ile eğitim veriyor. Ayrıca Atatürk İlkeleri ve İnkılap Tarihi, Avrupa Çalışmaları ve Sosyal Bilimler Enstitüleri bünyesindeki 92 yüksek lisans ve 44 doktora programında lisansüstü düzeyde eğitimlerini yürütüyor. Eğitim dili İngilizce olan Yeditepe Üniversitesi, kurulduğu günden bu yana 26 Ağustos Yerleşimi içerisinde Türkiye'nin dört bir yanından gelen gençleri hayata hazırlıyor. "
        android:textAlignment="center"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/locationButton"
        android:layout_width="67dp"
        android:layout_height="64dp"
        android:layout_marginBottom="16dp"
        android:src="@drawable/konum"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.651"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/kulupButton"
        android:layout_width="73dp"
        android:layout_height="64dp"
        android:layout_marginBottom="16dp"
        android:src="@drawable/club"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.079"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/profileButton"
        android:layout_width="73dp"
        android:layout_height="67dp"
        android:layout_marginBottom="16dp"
        android:src="@drawable/profil"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.952"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/chatButton2"
        android:layout_width="71dp"
        android:layout_height="65dp"
        android:layout_marginBottom="16dp"
        android:src="@drawable/livechat"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.37"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/takvimButton"
        android:layout_width="76dp"
        android:layout_height="73dp"
        android:layout_marginBottom="84dp"
        android:scaleType="centerInside"
        android:src="@drawable/takvim1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>